{
	"name": "tz-electron-vite",
	"private": true,
	"version": "0.0.0",
	"main": "dist-electron/main.js",
	"scripts": {
		"dev": "vite",
		"build": "vite build && electron-builder",
		"build:mac": "vite build && electron-builder --mac",
		"preview": "vite preview"
	},
	"dependencies": {
		"vue": "^3.3.4"
	},
	"devDependencies": {
		"@vitejs/plugin-vue": "^4.2.3",
		"cross-env": "^7.0.3",
		"electron": "^25.3.1",
		"electron-builder": "^24.4.0",
		"sass": "^1.64.1",
		"vite": "^4.4.5",
		"vite-plugin-electron": "^0.12.0",
		"vite-plugin-electron-renderer": "^0.14.5"
	},
	"build": {
		"asar": false,
		"extraFiles": [],
		"publish": [
			{
				"provider": "generic",
				"url": "http://127.0.0.1"
			}
		],
		"productName": "electron-vite-template",
		"appId": "",
		"directories": {
			"output": "build"
		},
		"files": [
			"dist-electron",
			"dist"
		],
		"dmg": {
			"contents": [
				{
					"x": 410,
					"y": 150,
					"type": "link",
					"path": "/Applications"
				},
				{
					"x": 130,
					"y": 150,
					"type": "file"
				}
			]
		},
		"mac": {
			"icon": "build/icons/icon.icns",
            "target":[
                "dmg"
            ]
		},
		"win": {
			"icon": "build/icons/icon.ico",
			"target": [
				{
					"target": "nsis",
					"arch": [
						"x64",
                        "ia32"
					]
				}
			]
		},
		"linux": {
			"target": "deb",
			"icon": "build/icons"
		}
	}
}
